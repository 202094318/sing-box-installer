{
    "log": {
      "level": "trace",
      "timestamp": true
    },
    "inbounds": [
      {
        "type": "vmess",
        "tag": "vmess-in",
        "listen": "0.0.0.0",
        "listen_port": 8080,
        "users": [
          {
            "name": "<proxy_name>",
            "uuid": "<proxy_uuid>"
          }
        ],
        "multiplex": {
          "enabled": true
        },
        "transport": {
          "type": "ws",
          "path": "/download",
          "headers": {
            "Host": "download.windowsupdate.com"
          }
        }
      }
    ]
  }
